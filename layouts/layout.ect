<%if(@googlemaps):%>
    <script>
        window.ti = window.ti || {};

        <%if(@mapConfig):%>
        var mapConfig = <%-@mapConfig%>
        <%end%>
        
        var centerLL =  {lat: <%-@site.coordinates.lat%>, lng: <%-@site.coordinates.lng%>};
        var zoom = 14;
        if(mapConfig) {
            centerLL = mapConfig.center;
            zoom = mapConfig.zoom;
        }
        function initMap() {
            window.ti.map = new google.maps.Map(document.getElementById('map'), {
            center: centerLL,
            zoom: zoom,
            scrollwheel: false
            });
            google.maps.event.addListenerOnce(map, 'tilesloaded', function(){
                 $('.map-overlay').fadeOut(1000,function(){
                    var marker = new google.maps.Marker({
                        position: centerLL,
                        map: map,
                        animation: google.maps.Animation.DROP
                    });
                 })
            });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDFO-sbwaYY3QEZ5WtPmWg4XZVbOodk2U8&callback=initMap" async defer></script>
<%end%>


